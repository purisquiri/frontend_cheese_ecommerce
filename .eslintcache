    password,\n        },\n      }),\n    })\n      .then((resp) => resp.json())\n      .then((data) => {\n        if (data.errors) throw data.errors;\n        else return data;\n      })\n      .then((data) => {\n        // console.log(data);\n        localStorage.setItem(\"token\", data.jwt);\n\n        if (data.user) {\n          handleUser(data.user);\n          history.push(\"/home\");\n          window.location.reload();\n        } else {\n          alert(\"Please fill in empty fields\");\n        }\n      })\n      .catch((errors) => {\n        setErrors(errors);\n        console.error(errors);\n      });\n  };\n\n  return (\n    <form className=\"container py-5\" onSubmit={(event) => handleSubmit(event)}>\n      <div className=\"ol-10 mx-auto text-center col-md-6 my-3\">\n        <h3>Sign Up</h3>\n\n        <div className=\"form-group\">\n          <label>Name</label>\n          <input\n            type=\"text\"\n            className=\"form-control\"\n            placeholder=\"User name\"\n            value={name}\n            onChange={(event) => setUsername(event.target.value)}\n          />\n        </div>\n\n        <div className=\"form-group\">\n          <label>Email address</label>\n          <input\n            type=\"email\"\n            className=\"form-control\"\n            placeholder=\"Enter email\"\n            value={email}\n            onChange={(event) => setEmail(event.target.value)}\n          />\n        </div>\n\n        <div className=\"form-group\">\n          <label>Password</label>\n          <input\n            type=\"password\"\n            className=\"form-control\"\n            placeholder=\"Enter password\"\n            value={password}\n            onChange={(event) => setPassword(event.target.value)}\n          />\n        </div>\n        {/* <Link to=\"/\"> */}\n        <ButtonContainer>Create Account</ButtonContainer>\n        {/* </Link> */}\n\n        <p className=\"forgot-password text-right\">\n          Already registered <a href=\"/login\">sign in?</a>\n        </p>\n      </div>\n    </form>\n  );\n}\n",["74","75"],"/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/components/Login.js",["76"],"/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/index.js",["77"],"/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/components/ProductList.js",["78"],"import React, { Component } from \"react\";\nimport Product from \"./Product\";\nimport Title from \"./Title\";\nimport { ProductConsumer } from \"../context\";\n\nexport default class ProductList extends Component {\n  render() {\n    return (\n      <React.Fragment>\n        <div className=\"py-5\">\n          <div className=\"container\">\n            {/* <Title name=\"our\" title=\"products\" /> */}\n\n            <div className=\"row\">\n              <ProductConsumer>\n                {(value) => {\n                  return value.products.map((product) => {\n                    return <Product key={product.id} product={product} />;\n                  });\n                }}\n              </ProductConsumer>\n            </div>\n          </div>\n        </div>\n      </React.Fragment>\n    );\n  }\n}\n","/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/components/Cart/Cart.js",[],"/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/components/Navbar.js",[],"/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/components/Home.js",[],"/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/context.js",[],"/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/userContext.js",[],["79","80"],"/Users/mdorigo/Development/final_proyect_flatiron/project/frontend-cheese-ecommerce/src/components/Details.js",[],"/Users/mdorigo/Development/final_proyect_flati